- content_for :css do
  = stylesheet_link_tag "information"

%div{class: 'grid_16'}
  = cms_region(@web_parts, 'information.tournaments', :div)

%div.grid_16#age_group_chart
  %h2
    %center
      %a{:href => '#'}Click here to view the Age Group Chart

%div{class: 'grid_16 box'}
  = cms_region(@web_parts, 'information.tournaments.mission_bell', :div)

%div{class: 'grid_16 box'}
  = cms_region(@web_parts, 'information.tournaments.premier_challenge', :div)

%div{class: 'grid_16'}
  = cms_region(@web_parts, 'information.tournaments.footer', :div)

%div{:id => "dialog", :title => "Age Group Chart"}

- content_for :javascript do
  = javascript_include_tag "information.tournaments"
  = javascript_include_tag "tryouts"
  %script
    $(document).ready(soccer.tryouts.show_age_group_chart($('#dialog')));