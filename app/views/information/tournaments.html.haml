- content_for :css do
  = stylesheet_link_tag "information"

%div.grid_16{ editable_attributes(@part_name_overview) }
  = editable_content @part_name_overview

%div.grid_16#age_group_chart
  %h2
    %center
      %a{:href => '#'}Click here to view the Age Group Chart

%div.grid_16.box{ editable_attributes(@part_name_mission_bell) }
  = editable_content(@part_name_mission_bell)

%div.grid_16.box{ editable_attributes(@part_name_premier_challenge) }
  = editable_content(@part_name_premier_challenge)

%div.grid_16{ editable_attributes(@part_name_footer)  }
  = editable_content @part_name_footer

%div{:id => "dialog", :title => "Age Group Chart"}

- content_for :javascript do
  = javascript_include_tag "information.tournaments"
  = javascript_include_tag "tryouts"
  %script
    $(document).ready(soccer.tryouts.show_age_group_chart($('#dialog')));